<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.devfun.settingweb_boot.dao.StatisticMapper">

    <select id="selectYearLogin" parameterType="java.lang.String" resultType="com.devfun.settingweb_boot.dto.StatisticDto">
        SELECT COUNT(*) AS totCnt
        FROM statistic.requestInfo ri
        WHERE LEFT(ri.createDate, 2) = #{year};
    </select>

</mapper>
